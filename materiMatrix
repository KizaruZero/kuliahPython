# perkalian 3x3 matriks
matr1 = [
    [1, 0, 1],
    [0, 0, 1],
    [0, 0, 1]
]

matr2 = [
    [0, 1, 0],
    [1, 1, 0],
    [1, 1, 0]
]

result = [[0, 0, 0] for _ in range(3)]  # Initialize a 3x3 matrix with zeros

# Perform matrix multiplication
for i in range(3):
    for j in range(3):
        for k in range(3):
            result[i][j] += matr1[i][k] * matr2[k][j]

# Print the result matrix
